{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/frontend/assets/$1"
        },
        {
            "src": "/(.*).(js|css|ico)",
            "dest": "/frontend/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/index.html"
        }
    ]
}
